use 'libs/tests/common.baabnq';
use 'libs/math.baabnq';

lab Test::Add;
    push 4;
    push 3;
    sub Math::Add;
    pull _v;
    jump fail ~ _v != 7;

    push 0;
    push 0;
    sub Math::Add;
    pull _v;
    jump fail ~ _v != 0;    

    push 1 << 15;
    push 1 << 15;
    sub Math::Add;
    pull _v;
    jump fail ~ _v != 0;    

    jump succ;

lab Test::Sub;
    push 4;
    push 3;
    sub Math::Sub;
    pull _v;
    jump fail ~ _v != 1;

    push 0;
    push 0;
    sub Math::Sub;
    pull _v;
    jump fail ~ _v != 0;

    push 1;
    push 2;
    sub Math::Sub;
    pull _v;
    jump fail ~ _v != (1 << 16) - 1;

    jump succ;


lab Test::Mul;
    push 4;
    push 3;
    sub Math::Mul;
    pull _v;
    jump fail ~ _v != 12;
    
    push 0;
    push 3;
    sub Math::Mul;
    pull _v;
    jump fail ~ _v != 0;

    push 4;
    push 0;
    sub Math::Mul;
    pull _v;
    jump fail ~ _v != 0;

    push (1 << 16) - 1;
    push (1 << 16) - 1;
    sub Math::Mul;
    pull _v;
    jump fail ~ _v != 1;
    
